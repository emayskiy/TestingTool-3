  <attributes>
    <name>ПланировщикЗаданий</name>
    <title>
      <key>ru</key>
      <value>Планировщик заданий</value>
    </title>
    <id>2</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>ПланировщикЗаданий.Задание</segments>
    </notDefaultUseAlwaysAttributes>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	Планировщик.Задание КАК Задание,
	Планировщик.ВремяНачала КАК ВремяНачала,
	Планировщик.ВремяОкончания КАК ВремяОкончания,
	Планировщик.Включено КАК Включено,
	Планировщик.Комментарий КАК Комментарий,
	Истина как Планировщик,
	ВЫБОР
		КОГДА ЕСТЬNULL(Запрос1.Задание, НЕОПРЕДЕЛЕНО) = НЕОПРЕДЕЛЕНО
			ТОГДА -1
		ИНАЧЕ 2
	КОНЕЦ КАК ВРаботе,
	ЕСТЬNULL(Запрос1.КоличествоЭтапов, 0) КАК КоличествоЭтапов,
	ЕСТЬNULL(Запрос1.КоличествоВыполненныхЭтапов, 0) КАК КоличествоВыполненныхЭтапов,
	ВЫБОР
		КОГДА ЕСТЬNULL(Запрос1.КоличествоЭтапов, 0) = 0
			ТОГДА 0
		ИНАЧЕ ВЫРАЗИТЬ(ЕСТЬNULL(Запрос1.КоличествоВыполненныхЭтапов, 0) / ЕСТЬNULL(Запрос1.КоличествоЭтапов, 0) * 100 КАК ЧИСЛО(10, 0))
	КОНЕЦ КАК ПроцентВыполненияЭтапов
ИЗ
	РегистрСведений.Планировщик КАК Планировщик
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			Пулл.Задание КАК Задание,
			КОЛИЧЕСТВО(Пулл.ПорядокВыполнения) КАК КоличествоЭтапов,
			СУММА(ВЫБОР
					КОГДА Пулл.ЭтапВыполнен = ИСТИНА
						ТОГДА 1
					ИНАЧЕ 0
				КОНЕЦ) КАК КоличествоВыполненныхЭтапов
		ИЗ
			РегистрСведений.СоставЗаданийПулл КАК Пулл
		
		СГРУППИРОВАТЬ ПО
			Пулл.Задание) КАК Запрос1
		ПО (Запрос1.Задание = Планировщик.Задание)
ГДЕ
	(Планировщик.Задание.ГруппаЗадания = &amp;ГруппаЗадания
			ИЛИ &amp;ГруппаЗадания = ЗНАЧЕНИЕ(Справочник.ГруппыЗаданий.ПустаяСсылка))

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Задания.Ссылка,
	NULL,
	NULL,
	ЛОЖЬ,
	Задания.Комментарий,
	Ложь,
	ВЫБОР
		КОГДА ЕСТЬNULL(Запрос1.Задание, НЕОПРЕДЕЛЕНО) = НЕОПРЕДЕЛЕНО
			ТОГДА -1
		ИНАЧЕ 2
	КОНЕЦ,
	ЕСТЬNULL(Запрос1.КоличествоЭтапов, 0),
	ЕСТЬNULL(Запрос1.КоличествоВыполненныхЭтапов, 0),
	ВЫБОР
		КОГДА ЕСТЬNULL(Запрос1.КоличествоЭтапов, 0) = 0
			ТОГДА 0
		ИНАЧЕ ВЫРАЗИТЬ(ЕСТЬNULL(Запрос1.КоличествоВыполненныхЭтапов, 0) / ЕСТЬNULL(Запрос1.КоличествоЭтапов, 0) * 100 КАК ЧИСЛО(10, 0))
	КОНЕЦ
ИЗ
	Справочник.Задания КАК Задания
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ РАЗЛИЧНЫЕ
			Т.Задание КАК Задание
		ИЗ
			РегистрСведений.Планировщик КАК Т) КАК Планировщик
		ПО (Планировщик.Задание = Задания.Ссылка)
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			Пулл.Задание КАК Задание,
			КОЛИЧЕСТВО(Пулл.ПорядокВыполнения) КАК КоличествоЭтапов,
			СУММА(ВЫБОР
					КОГДА Пулл.ЭтапВыполнен = ИСТИНА
						ТОГДА 1
					ИНАЧЕ 0
				КОНЕЦ) КАК КоличествоВыполненныхЭтапов
		ИЗ
			РегистрСведений.СоставЗаданийПулл КАК Пулл
		
		СГРУППИРОВАТЬ ПО
			Пулл.Задание) КАК Запрос1
		ПО (Запрос1.Задание = Задания.Ссылка)
ГДЕ
	(Задания.ГруппаЗадания = &amp;ГруппаЗадания
			ИЛИ &amp;ГруппаЗадания = ЗНАЧЕНИЕ(Справочник.ГруппыЗаданий.ПустаяСсылка))
	И &amp;ВсеЗадания = ИСТИНА
	И Задания.ЭтоГруппа = ЛОЖЬ
	И Планировщик.Задание ЕСТЬ NULL</queryText>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
  </attributes>
